<body class="hold-transition skin-blue sidebar-mini">

	<div class="wrapper">

		<jsp:include page="header.jsp"></jsp:include>

		<jsp:include page="aside.jsp"></jsp:include>

		<div class="content-wrapper">

			<section class="content-header">
			<h1>
				Order management <small>All orders</small>
			</h1>
			<ol class="breadcrumb">
				<li><a href="${pageContext.request.contextPath}/index.jsp"><i
						class="fa fa-dashboard"></i> Home</a></li>
				<li><a href="${pageContext.request.contextPath}/order/findAll.do">Order management</a></li>
				<li class="active">Edit order</li>
			</ol>
			</section>

			<section class="content">
			<form action="${pageContext.request.contextPath}/order/save" method="post">
				
				<div class="panel panel-default">
					<div class="panel-heading">Order info</div>
					<div class="row data-type">

						<div class="col-md-2 title">Order number</div>
						<div class="col-md-4 data">
							<input type="text" class="form-control" placeholder="order number"
								 name="orderNum">
						</div>

						<div class="col-md-2 title">Order time</div>
						<div class="col-md-4 data">
							<div class="input-group date">
								<div class="input-group-addon">
									<i class="fa fa-calendar"></i>
								</div>
								<input type="text" class="form-control pull-right"
									id="datepicker-a3" name="orderTime" >
							</div>
						</div>
						<div class="col-md-2 title">Number of people</div>
						<div class="col-md-4 data">
							<input type="text" class="form-control" placeholder="number of people"
								 name="peopleCount">
						</div>

						<div class="col-md-2 title">Payment method</div>
						<div class="col-md-4 data">
							<select class="form-control select2" style="width: 100%"
								name="payType">
								<option value="0" selected="selected">paypal</option>
								<option value="1">zelle</option>
								<option value="2">others</option>
							</select>
						</div>
						
						<div class="col-md-2 title">Order status</div>
						<div class="col-md-4 data">
							<select class="form-control select2" style="width: 100%"
								name="orderStatus">
								<option value="0" selected="selected">unpaid</option>
								<option value="1">paid</option>
							</select>
						</div>
						
						<div class="col-md-2 title">select product</div>
						<div class="col-md-4 data">
							<select class="form-control select2" style="width: 100%"
								name="product.id">
								<c:forEach items="${ productList }" var="p">
									<option value="${ p.id }" >${ p.productName }</option>
								</c:forEach>
							</select>
						</div>

						<div class="col-md-2 title rowHeight2x">Order description</div>
						<div class="col-md-10 data rowHeight2x">
							<textarea class="form-control" rows="3" placeholder="order description" name="orderDesc">
						</textarea>
						</div>

					</div>
				</div>

				<div class="box-tools text-center">
					<button type="submit" class="btn bg-maroon">Save</button>
					<button type="button" class="btn bg-default"
						onclick="history.back(-1);">return</button>
				</div>
			</form>
			</section>


		</div>

